<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="Default Design"
        sling:resourceType="wcm/core/components/designer">
        <page jcr:primaryType="nt:unstructured">
            <par jcr:primaryType="nt:unstructured">
                <video
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="foundation/components/video"
                    profiles="[hq,firefoxhq]"/>
            </par>
        </page>
        <segment-page jcr:primaryType="nt:unstructured">
            <traits jcr:primaryType="nt:unstructured">
                <orpar
                    jcr:primaryType="nt:unstructured"
                    components="group:ContextHub Segmentation">
                    <section jcr:primaryType="nt:unstructured"/>
                </orpar>
                <andpar
                    jcr:primaryType="nt:unstructured"
                    components="group:ContextHub Segmentation">
                    <section jcr:primaryType="nt:unstructured"/>
                </andpar>
            </traits>
        </segment-page>
        <segmentpage-campaigns jcr:primaryType="nt:unstructured">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/parsys"
                components="[/libs/cq/personalization/components/traits/logic/or]">
                <section jcr:primaryType="nt:unstructured"/>
            </par>
            <orpar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="/libs/cq/personalization/components/traits/logic/or"
                components="[group:Segmentation - Adobe Campaign]">
                <section jcr:primaryType="nt:unstructured"/>
            </orpar>
        </segmentpage-campaigns>
        <sequencechannel jcr:primaryType="nt:unstructured">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="screens/core/components/sequencechannel/sequence"
                components="group:Screens">
                <section jcr:primaryType="nt:unstructured"/>
            </par>
        </sequencechannel>
    </jcr:content>
    <static.css/>
    <logo.png/>
    <rep:policy/>
</jcr:root>
