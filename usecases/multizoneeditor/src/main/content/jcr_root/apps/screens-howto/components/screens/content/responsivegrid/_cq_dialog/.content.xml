<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Layout Container"
    sling:resourceType="screens/core/components/dialogschedule">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs"
        size="L">
        <items jcr:primaryType="nt:unstructured"
               sling:hideChildren="[column]">
            <container
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Layout Container"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <height
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                emptyOption="${Boolean}true"
                                emptyText="Height"
                                name="./cq:responsive/default/height">
                                <items jcr:primaryType="nt:unstructured">
                                    <halves
                                        jcr:primaryType="nt:unstructured"
                                        disabled="{Boolean}true"
                                        text="Halves"/>
                                    <h100
                                        jcr:primaryType="nt:unstructured"
                                        text="Full"
                                        value="100"/>
                                    <h50
                                        jcr:primaryType="nt:unstructured"
                                        text="1/2"
                                        value="50"/>
                                    <thirds
                                        jcr:primaryType="nt:unstructured"
                                        disabled="{Boolean}true"
                                        text="Thirds"/>
                                    <h66
                                        jcr:primaryType="nt:unstructured"
                                        text="2/3"
                                        value="66"/>
                                    <h33
                                        jcr:primaryType="nt:unstructured"
                                        text="1/3"
                                        value="33"/>
                                    <quarters
                                        jcr:primaryType="nt:unstructured"
                                        disabled="{Boolean}true"
                                        text="Quartes"/>
                                    <h75
                                        jcr:primaryType="nt:unstructured"
                                        text="3/4"
                                        value="75"/>
                                    <h25
                                        jcr:primaryType="nt:unstructured"
                                        text="1/4"
                                        value="25"/>
                                    <fifths
                                        jcr:primaryType="nt:unstructured"
                                        disabled="{Boolean}true"
                                        text="Fifths"/>
                                    <h80
                                        jcr:primaryType="nt:unstructured"
                                        text="4/5"
                                        value="80"/>
                                    <h60
                                        jcr:primaryType="nt:unstructured"
                                        text="3/5"
                                        value="60"/>
                                    <h40
                                        jcr:primaryType="nt:unstructured"
                                        text="2/5"
                                        value="40"/>
                                    <h20
                                        jcr:primaryType="nt:unstructured"
                                        text="1/5"
                                        value="20"/>
                                    <sixths
                                        jcr:primaryType="nt:unstructured"
                                        disabled="{Boolean}true"
                                        text="Sixths"/>
                                    <h83
                                        jcr:primaryType="nt:unstructured"
                                        text="5/6"
                                        value="83"/>
                                    <h16
                                        jcr:primaryType="nt:unstructured"
                                        text="1/6"
                                        value="16"/>
                                    <eigths
                                        jcr:primaryType="nt:unstructured"
                                        disabled="{Boolean}true"
                                        text="Eigths"/>
                                    <h87_5
                                        jcr:primaryType="nt:unstructured"
                                        text="7/8"
                                        value="87_5"/>
                                    <h62_5
                                        jcr:primaryType="nt:unstructured"
                                        text="5/8"
                                        value="62_5"/>
                                    <h37_5
                                        jcr:primaryType="nt:unstructured"
                                        text="3/8"
                                        value="37_5"/>
                                    <h12_5
                                        jcr:primaryType="nt:unstructured"
                                        text="1/8"
                                        value="12_5"/>
                                    <tenths
                                        jcr:primaryType="nt:unstructured"
                                        disabled="{Boolean}true"
                                        text="Tenths"/>
                                    <h90
                                        jcr:primaryType="nt:unstructured"
                                        text="9/10"
                                        value="90"/>
                                    <h70
                                        jcr:primaryType="nt:unstructured"
                                        text="7/10"
                                        value="70"/>
                                    <h30
                                        jcr:primaryType="nt:unstructured"
                                        text="3/10"
                                        value="30"/>
                                    <h10
                                        jcr:primaryType="nt:unstructured"
                                        text="1/10"
                                        value="10"/>
                                </items>
                            </height>
                        </items>
                    </column>

                </items>
            </container>
        </items>
    </content>
</jcr:root>
